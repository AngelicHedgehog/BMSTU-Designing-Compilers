*$FROM Parser/Block/Block
$EXTERN Block;

Program {
    /* Program -> Block */
    () (Block e.B) e.Tokens t.Errs
        = <Program ((Block e.B)) e.Tokens t.Errs>;
    () e.Tokens t.Errs
        = <Program () <Block () e.Tokens t.Errs>>;

    (t.Block) e.Tokens t.Errs
        = (Program (Block) t.Block) e.Tokens t.Errs;
}
